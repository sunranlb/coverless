{
    "name": "Coverless",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "Remove any cover that block your work automatically.",
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ]
}